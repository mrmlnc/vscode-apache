{
  "name": "vscode-apache",
  "displayName": "Apache Conf",
  "description": "Syntax highlighter for Apache configuration files",
  "version": "1.1.0",
  "publisher": "mrmlnc",
  "license": "MIT",
  "engines": {
    "vscode": "^1.0.0"
  },
  "icon": "icon.png",
  "homepage": "https://github.com/mrmlnc/vscode-apache/blob/master/README.md",
  "repository": {
    "type": "git",
    "url": "https://github.com/mrmlnc/vscode-apache"
  },
  "keywords": [
    "apache",
    "http",
    "https",
    "syntax",
    "highlight"
  ],
  "categories": [
    "Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "apacheconf",
      "aliases": ["Apache", "apache"],
      "extensions": [
        "conf",
        ".conf",
        "htaccess",
        ".htaccess",
        "htgroups",
        ".htgroups",
        "htpasswd",
        ".htpasswd"
      ],
      "configuration": "./apache.configuration.json"
    }],
    "grammars": [{
      "language": "apacheconf",
      "scopeName": "source.apacheconf",
      "path": "./syntaxes/Apache.tmLanguage"
    }]
  }
}
